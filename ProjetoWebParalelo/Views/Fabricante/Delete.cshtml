@model Modelo.Cadastro.Fabricante

@{
    ViewData["Title"] = "Delete";
}

<h3>Você quer mesmo remover este Fabricante @Html.DisplayFor(model => model.Nome)?</h3>
<div>

    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Nome)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nome)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Nacionalidade)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nacionalidade)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="FabricanteId" />
        <tr>
            <td>
                <input type="submit" value="Delete" class="btn btn-danger" />
            </td>
            <td>
                <a asp-action="Index" class="btn btn-outline-primary">Voltar</a>
            </td>
        </tr>
    </form>
</div>
