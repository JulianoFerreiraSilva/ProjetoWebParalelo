@model Modelo.Cadastro.Fabricante

@{
    ViewData["Title"] = "Detalhes Fabricante";
}

<div>
    <h4>Fabricante</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Nome)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nome)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Nacionalidade)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nacionalidade)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.FabricanteId" class="btn btn-outline-primary">Editar</a> |
    <a asp-action="Index" class="btn btn-outline-success">Voltar</a>
</div>
